{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.php",
        "use": "vercel-php@0.7.1"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*).php",
        "dest": "/api/$1.php"
      }
    ],
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    }
  }